{
  "name": "inferno-rollup-boilerplate",
  "version": "1.0.0",
  "description": "Inferno, Rollup.js config",
  "main": "rollup.config.js",
  "repository": "https://github.com/balajmarius/inferno-rollup-boilerplate.git",
  "author": "Balaj Marius",
  "license": "MIT",
  "scripts": {
    "watch": "rollup -c -w",
    "sync": "browser-sync start --s --index 'static/index.html' --files 'build/**' --no-notify",
    "build": "NODE_ENV=production && rollup -c",
    "start:development": "npm-run-all --parallel sync watch"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inferno": "^3.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "browser-sync": "^2.18.8",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-inferno": "^6.10.4",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.2.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sass": "^0.4.10",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "inferno": "^1.6.2",
    "inferno-component": "^1.6.2"
  }
}
